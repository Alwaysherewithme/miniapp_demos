{
  "name": "abc-client",
  "version": "1.5.0",
  "description": "Web client for abc",
  "main": "index.js",
  "author": "Alloy",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run app-profile & npm run app-info",
    "app-profile": "webpack-dev-server --host 0.0.0.0 --config webpack/profile.development.js --progress",
    "app-info": "webpack-dev-server --host 0.0.0.0 --config webpack/infoCentre.development.js --progress",
    "app-profile-no-auth": "webpack-dev-server --config webpack/profile.test.js --progress --env.EMPLOYEE_ID",
    "app-info-no-auth": "webpack-dev-server --config webpack/infoCentre.test.js --progress --env.EMPLOYEE_ID",
    "start-no-auth": "npm run app-profile-no-auth & npm run app-info-no-auth",
    "build": "webpack --config webpack/production.js",
    "lint": "./node_modules/.bin/eslint .",
    "lint-fix": "./node_modules/.bin/eslint --fix .",
    "docs": "cd src/js/api && esdoc"
  },
  "jest": {
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "globals": {
      "__TEST__": true
    },
    "testMatch": [
      "**/__tests__/**/*.js?(x)"
    ],
    "verbose": true
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "browser",
      "es6",
      "jest"
    ],
    "ignore": [
      "/webpack/",
      "/src/assets/"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
